{
	"Filter": {
	  "prefix": "fil",
	  "body": [
		"const arr = ['a', 'b', 'c'];",
		"const result = arr.filter((str) => str === 'b');",
		"console.log(result); // ['b']",
	  ]
	},
	"Find": {
	  "prefix": "fin",
	  "body": [
		"const arr = [1, 2, 3];",
		"const result = arr.find((num) => num > 1);",
		"console.log(result); // 2",
	  ]
	},
	"Map": {
	  "prefix": "map",
	  "body": [
		"const arr = [1, 2, 3];",
		"const result = arr.map((num) => num * 2);",
		"console.log(result); // [2, 4, 6]",
	  ]
	},
