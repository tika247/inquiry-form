{
	// Place your snippets for scss here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"flex-row": {
		"prefix": "fl",
		"body": [
			"display: flex;",
			"justify-content: center;",
			"align-items: center;"
		]
	},
	"flex-column": {
		"prefix": "fc",
		"body": [
			"display: flex;",
			"flex-direction: column;",
			"justify-content: center;",
			"align-items:center;"
		]
	},
	"trans": {
		"prefix": "trans",
		"body": [
			"top: 50%;",
			"left: 50%;",
			"transform: translate(-50%, -50%);"
		]
	},
	"divide": {
		"prefix": "/",
		"body": [
			"/750*100vw"
		]
	},
	"pseudo-element": {
		"prefix": "::",
		"body": [
			"::after{",
			"width: $1px;",
			"content: \"\";",
			"position: absolute;",
			"top: 50%;",
			"transform: translateY(-50%);",
			"right: $2px;",
			"cursor: pointer;",
			"}"
		]
	},
	"absolute": {
		"prefix": "a",
		"body": [
			"width: $1px;",
			"content: \"\";",
			"position: absolute;",
			"top: 50%;",
			"transform: translateY(-50%);",
			"$2: $3px;",
			"cursor: pointer;"
		]
	},
	"devide": {
		"prefix": "/",
		"body": [
			"/750*100vw;"
		]
	},
	"include": {
		"prefix": "s",
		"body": [
			"@include sp{",
			"$1",
			"}"
		]
	},
	"include-tab": {
		"prefix": "t",
		"body": [
			"@include tab{",
			"$1",
			"}"
		]
	},
	"vw": {
		"prefix": "v",
		"body": [
			"@include vw($1);"
		]
	},
	"margin0auto": {
		"prefix": "m",
		"body": [
			"margin: 0 auto;"
		]
	},
	"background-image": {
		"prefix": "b",
		"body": [
			"width:;",
			"height:;",
			"background-image: url('/assets/img/$1');",
			"background-size: contain;",
			"background-repeat: no-repeat;",
			"background-position: center center;"
		]
	},
	"element-center": {
		"prefix": "c",
		"body": [
			"width: $1;",
			"position: absolute;",
			"top: 50%;",
			"left: 50%;",
			"transform: translate(-50%,-50%);"
		]
	},
	"element-fix": {
		"prefix": "fix",
		"body": [
			"width: $1;",
			"max-width: 90vw;",
			"height: 90vh;",
			"position: fix;",
			"top: 50%;",
			"left: 50%;",
			"transform: translate(-50%,-50%);"
		]
	},
	"font-size": {
		"prefix": "fs",
		"body": [
			"@include fs($1);"
		]
	},
	"pc": {
		"prefix": "p",
		"body": [
			"@include pc{",
			"$1",
			"}"
		]
	},
}